{{!
    Page header.
}}
{{{ output.doctype }}}
<html {{{ output.htmlattributes }}}>
<head>
    <title>{{{ output.page_title }}}</title>

    {{#themeauthor}}
    <!-- * * * * * * *

      Theme: BAZ Moodle Theme
      Author: Marcin Czaja - Rosea Themes (rosea.io)
      Version: 1.5.2

      Copyright Â© 2021 - 2022

    * * * * * * * -->
    {{/themeauthor}}

    {{#seometadesc}}<meta name="description" content="{{seometadesc}}">{{/seometadesc}}
    <meta name="theme-color" content="{{seothemecolor}}{{^seothemecolor}}#00665F{{/seothemecolor}}">

    <link rel="shortcut icon" href="{{{ output.favicon }}}" />
    {{#seoappletouchicon}}<link rel="apple-touch-icon" href="{{seoappletouchicon}}">{{/seoappletouchicon}}

    {{{ output.standard_head_html }}}
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="{{#googlefonturl}}{{{googlefonturl}}}{{/googlefonturl}}{{^googlefonturl}}https://fonts.googleapis.com/css2?family=Inter:wght@400;500;700&display=swap{{/googlefonturl}}" rel="stylesheet">

    <!-- Simplebar -->
    <link rel="stylesheet" href="{{siteurl}}/theme/baz/addons/simplebar/simplebar.css"/>
    <script src="{{siteurl}}/theme/baz/addons/simplebar/simplebar.min.js"></script>

    <!-- Swiper JS -->
    <script src="{{siteurl}}/theme/baz/addons/swiper/swiper-bundle.min.js"></script>

</head>
